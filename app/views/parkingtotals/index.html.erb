<p id="notice"><%= notice %></p>

<h1>Listing Parkingtotals</h1>

<table>
  <thead>
    <tr>
      <th>Zone</th>
      <th>Free</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parkingtotals.each do |parkingtotal| %>
      <tr>
        <td><%= parkingtotal.zone %></td>
        <td><%= parkingtotal.free %></td>
        <td><%= link_to 'Show', parkingtotal %></td>
        <td><%= link_to 'Edit', edit_parkingtotal_path(parkingtotal) %></td>
        <td><%= link_to 'Destroy', parkingtotal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parkingtotal', new_parkingtotal_path %>
